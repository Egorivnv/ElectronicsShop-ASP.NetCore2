<div id="accordion" class="col-md-3">
    <div class="card">
        <div class="card-header  text-center">
            <button class="btn btn-link font-weight-bold text-dark" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                FILTER BY PARAMETERS
            </button>
        </div>
        <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordion">
            <div class="card-body">
                <ul class="list-group list-group-flush">
                    <li class="list-group-item ">
                        <form id="filterForm ">
                            <ul class="list-group ">
                                <li class="list-group-item">
                                    <div class="row" style="position:relative; bottom: 15px">
                                        <span class="font-weight-bold ">Price:</span>
                                    </div>
                                    <div class="row" style="position:relative; bottom: 15px">
                                        <div class="col-sm-6 pull-left">
                                            <label>
                                                from:<input class="w-100" type="text" name="priceFrom" />
                                            </label>
                                        </div>
                                        <div class="col-sm-6">
                                            <label>
                                                to:<input class="w-100" type="text" name="priceTo" />
                                            </label>
                                        </div>
                                    </div>
                                </li>
                                <li class="list-group-item">
                                    <div class="row ">
                                        <span class="font-weight-bold">Display diagonal, inces:</span>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-6 pull-left">
                                            <label>
                                                from:<input class="w-100" type="text" name="diagFrom" />
                                            </label>
                                        </div>
                                        <div class="col-sm-6">
                                            <label>
                                                to:<input class="w-100" type="text" name="diagTo" />
                                            </label>
                                        </div>
                                    </div>
                                </li>
                                <li class="list-group-item">
                                    <div class="row ">
                                        <span class="font-weight-bold">RAM, GB:</span>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-6 pull-left">
                                            <label>
                                                from:<input class="w-100" type="text" name="ramFrom" />
                                            </label>
                                        </div>
                                        <div class="col-sm-6">
                                            <label>
                                                to:<input class="w-100" type="text" name="ramTo" />
                                            </label>
                                        </div>
                                    </div>
                                </li>
                                <li class="list-group-item">
                                    <div class="row ">
                                        <span class="font-weight-bold">Memory, GB:</span>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-6 pull-left">
                                            <label>
                                                from:<input class="w-100" type="text" name="ramFrom" />
                                            </label>
                                        </div>
                                        <div class="col-sm-6">
                                            <label>
                                                to:<input class="w-100" type="text" name="ramTo" />
                                            </label>
                                        </div>
                                    </div>
                                </li>
                                <li class="list-group-item">
                                    <div class="container-fluid">
                                        <div class="row ">
                                            <span class="font-weight-bold">Color:</span>
                                        </div>
                                        <div class="form-check">
                                            <input class="form-check-input" type="checkbox" value="" name="colorBlack" id="colorBlack">
                                            <label class="form-check-label" for="colorBlack">
                                                Black
                                            </label>
                                        </div>
                                        <div class="form-check">
                                            <input class="form-check-input" type="checkbox" value="" name="colorSilver" id="colorSilver">
                                            <label class="form-check-label" for="colorSilver">
                                                Silver
                                            </label>
                                        </div>
                                        <div class="form-check">
                                            <input class="form-check-input" type="checkbox" value="" name="colorGold" id="colorGold">
                                            <label class="form-check-label" for="colorGold">
                                                Gold
                                            </label>
                                        </div>
                                        <div class="form-check">
                                            <input class="form-check-input" type="checkbox" value="" name="colorGold" id="colorGold">
                                            <label class="form-check-label" for="colorGold">
                                                Grey
                                            </label>
                                        </div>
                                        <div class="form-check">
                                            <input class="form-check-input" type="checkbox" value="" name="colorRose" id="colorRose">
                                            <label class="form-check-label" for="colorRose">
                                                Rose
                                            </label>
                                        </div>
                                        <div class="form-check">
                                            <input class="form-check-input" type="checkbox" value="" name="colorBlue" id="colorBlue">
                                            <label class="form-check-label" for="colorBlue">
                                                White
                                            </label>
                                        </div>
                                    </div>
                                </li>
                                <li class="list-group-item text-center">
                                    <button class="btn btn-primary mr-1 mt-3" type="submit">Filter</button>
                                    <button class="btn btn-light ml-1 mt-3" id="resetFilter">Reset</button>
                                </li>
                            </ul>
                        </form>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
